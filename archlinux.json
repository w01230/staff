{
    "!root-password": "******",
    "audio": null,
    "bootloader": "systemd-bootctl",
    "filesystem": "f2fs",
    "harddrive": {
        "model": "VMware Virtual NVMe Disk",
        "path": "/dev/nvme0n1",
        "size": "128G"
    },
    "hostname": "archlinux",
    "kernels": [
        "linux-lts"
    ],
    "keyboard-language": "us",
    "mirror-region": {
        "China": {
            "https://mirrors.aliyun.com/archlinux/$repo/os/$arch": true,
            "https://mirrors.cloud.tencent.con/archlinux/$repo/os/$arch": true,
            "https://mirrors.ustc.edu.cn/archlinux/$repo/os/$arch": true
        }
    },
    "nic": {
        "nic": "ens160"
    },
    "ntp": true,
    "packages": [“net-tools”, “nfs-utils”, “samba”, “vim”, “open-vm-tools”],
    "profile": null,
    "script": "guided",
    "superusers": {},
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Asia/Shanghai",
    "users": {
         "jun": {
            "!password": "******"
        }       
    }
}
